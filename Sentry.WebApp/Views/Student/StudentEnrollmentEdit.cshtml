            // StudentMasterId ---
            var enrollmentStudentMasterIdDropDown = $('select#StudentMasterId');
            var studentMethod = '@Html.Raw(Url.Action("GetStudents", "DropDowns"))';

            $(enrollmentStudentMasterIdDropDown).select2({
                selectOnClose: false, // select currently highlighted option when menu is closed
                closeOnSelect: true, // close menu once item is selected
                allowClear: true,
                width: '100%',
                ajax: {
                    delay: 250,
                    url: studentMethod,
                    dataType: 'json',
                    contentType: "application/json; charset=utf-8",
                    data: function (params) {
                        var queryParams = {
                            term: params.term, // search term
                            _type: 'query_append',
                            selected: true,
                            page: params.page || 1
                        }
                        return queryParams;
                    },
                    processResults: function (data, params) {
                        params.page = params.page || 1;
                        return {
                            results: data,
                            pagination: {
                                more: (params.page * 5) < data.length
                            }
                        };
                    },
                    cache: true
                },
                placeholder: '-- Choose Student --',
                minimumInputLength: 3,
                //minimumResultsForSearch: 5,
                templateResult: function (data) {
                    var $result = $("<span></span>");
                    $result.text(data.text);
                    return $result;
                },
                templateSelection: function (data) {
                    return data.text;
                }
            });

            // Initialize select2 with Current Value
            var savedValue = '@Model.StudentId';
            console.log('Selected StudentId value: ' + savedValue);

            var masterId = @(Html.Raw(string.IsNullOrEmpty(Model.StudentMasterId) ? "null" : $"'{Model.StudentMasterId}'"));

            if (masterId != null) {
                var savedOption = new Option(savedValue, masterId, true);
                $(enrollmentStudentMasterIdDropDown).append(savedOption).trigger('change');
            };

            // Get/Set Current Drop-Down Value:
            $(enrollmentStudentMasterIdDropDown).on('change', function () {

                var currentEnrollmentStudentMasterIdSelectListValue = $(this).find(":selected").val();

                if (currentEnrollmentStudentMasterIdSelectListValue == null || currentEnrollmentStudentMasterIdSelectListValue == "") {
                    $('#StudentId').val(null);
                } else {
                    var url = '@Html.Raw(Url.Action("GetStudent", "Student"))' + '/' + currentEnrollmentStudentMasterIdSelectListValue;
                    $.getJSON(url, function (data) {
                        console.log(data);
                        console.log("success");
                        $('#StudentId').val(data.studentId);
                    });
                };
            });


