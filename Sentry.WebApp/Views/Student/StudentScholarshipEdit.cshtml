@model Sentry.WebApp.ViewModels.StudentScholarshipViewModel
@using Sentry.WebApp.Extensions;

@{
    ViewData["Title"] = Model.StudentId + " | " + Model.Title + " Details";
}

@if (Model != null)
{
    <ul class="page-title">
        <li>
            <h2>
                Date:
                <span title="Date of Integration: @Html.Raw(Model.CreatedOnDT.HasValue ? Model.CreatedOnDT.Value.ToString() : "N/A")"> @Html.Raw(Model.CreatedOnDT.HasValue ? Model.CreatedOnDT.Value.ToString("MM/dd/yyyy") : "N/A")</span>
            </h2>
        </li>
        <li>
            <h2>
                Source:
                <span title="Source System">@Html.Raw(Model.System ?? "N/A")</span>
            </h2>
        </li>
        <li>
            <h2>
                Source Record Id:
                @if (Model.System == "Lynx")
                {
                    <a href="@Model.SourceRecordUrl" title="View this record in @Model.System" class="" target="_blank">
                        <span>@Html.Raw(Model.SourceRecordId ?? "N/A")</span>
                    </a>
                }
                else
                {
                    @*<span title="Source System Record Id">@(Model.SourceRecordId != "" || Model.SourceRecordId != null ? Html.Raw(Model.SourceRecordId) : Html.Raw("N/A"))</span>*@
                    <span title="Source System Record Id">@Html.Raw(Model.SourceRecordId ?? "N/A")</span>
                }
            </h2>
        </li>
    </ul>

    <ol class="breadcrumb">
        <li><a asp-area="" asp-controller="Home" asp-action="Index">Sentry Home</a></li>
        <li><a asp-area="" asp-controller="Student" asp-action="Index">Student Dashboard</a></li>
        <li><a asp-area="" asp-controller="Student" asp-action="StudentScholarshipList">Record List</a></li>
        <li class="active">Record Details</li>
    </ol>

    <div id="content-container" class="content-container" role="main">
        <form asp-action="StudentScholarshipEdit" id="StudentScholarshipEditForm">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="SystemId" />
            <input type="hidden" asp-for="System" />
            <input type="hidden" asp-for="SourceRecordId" />
            <input type="hidden" asp-for="IntegrationId" />
            <input type="hidden" asp-for="Integration" />
            <input type="hidden" asp-for="CreatedOnDT" />
            <input type="hidden" asp-for="IsChanged" />
            @for (int i = 0; i <= Model.HistoryData.Count - 1; i++)
            {
                @Html.HiddenFor(m => m.HistoryData[i].HistoryDate)
            }

            <div class="panel panel-secondary">
                <div class="panel-heading">
                    <h3 class="panel-title">@Model.Title <span class="updatedFormAlert label label-warning hide">Modified</span></h3>
                    <div class="panel-toolbar">
                        <a href="#scholarshipScholarshipSourceDataModal" class="btn btn-sm btn-link" target="_blank" data-toggle="modal" title="View Source Data"><i role="img" class="fas fa-database fa-sm">&nbsp;</i>Source Data</a>
                    </div>
                </div>
                <div class="panel-body">

                    <div class="col-sm-12 col-md-12 panel-container">
                        <div class="panel panel-secondary">
                            <div class="panel-heading">
                                <h4 class="panel-title">Scholarship Details:</h4>
                                <div class="panel-toolbar">
                                    <button class="btn btn-sm btn-link infoLegend" data-toggle="tooltip" type="button" aria-expanded="false">History Legend <i role="img" class="fas fa-question-circle"></i></button>
                                </div>
                            </div>
                            <div class="panel-body">

                                <fieldset>
                                    <legend class="sr-only">Scholarship Details:</legend>
                                    <div class="col-xs-12 col-sm-6 col-lg-4">
                                        <!-- StudentMasterId -->
                                        <div class='form-group @string.Format("status{0}", Model.StudentMasterId_Status)'>
                                            <div class="pull-right">
                                                <input type="hidden" asp-for="StudentMasterId_BusinessName" />
                                                <input type="hidden" asp-for="StudentMasterId_BusinessDescription" />
                                                <input type="hidden" asp-for="StudentMasterId_Source" />
                                                <input type="hidden" asp-for="StudentMasterId_Status" />
                                                <input type="hidden" asp-for="StudentMasterId_OriginalValue" />
                                                <input type="hidden" asp-for="StudentId" />
                                                <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                        data-info-content-name="@Html.DisplayFor(m => m.StudentMasterId_BusinessName)"
                                                        data-info-content-description="@Html.DisplayFor(m => m.StudentMasterId_BusinessDescription)"
                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.StudentMasterId_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                                </button>
                                                <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#scholarshipStudentMasterIdHistoryModal" tabindex="-1"
                                                        data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].StudentMasterId_Status)"
                                                        data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                        data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].StudentMasterId)">
                                                    <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                                </button>
                                                <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                            </div>
                                            <label class="control-label" for="StudentMasterId">@Model.StudentMasterId_BusinessName: </label>
                                            <select asp-for="StudentMasterId" class="form-control" placeholder="@Model.StudentMasterId_BusinessName" data-disabled="@(Model.StudentMasterId_IsReadOnly.ToString())"></select>
                                            <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                            <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                            <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                            <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                            @Html.Raw(Model.StudentMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                            <span asp-validation-for="StudentMasterId" class="text-danger help-block"></span>
                                        </div>
                                    </div><!-- /.col-* -->
                                    <div class="col-xs-12 col-sm-6 col-lg-4">
                                        <!-- TermMasterId -->
                                        <div class='form-group @string.Format("status{0}", Model.TermMasterId_Status)'>
                                            <div class="pull-right">
                                                <input type="hidden" asp-for="TermMasterId_BusinessName" />
                                                <input type="hidden" asp-for="TermMasterId_BusinessDescription" />
                                                <input type="hidden" asp-for="TermMasterId_Source" />
                                                <input type="hidden" asp-for="TermMasterId_Status" />
                                                <input type="hidden" asp-for="AcademicCareerCode" />
                                                <input type="hidden" asp-for="TermCode" />
                                                <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                        data-info-content-name="@Html.DisplayFor(m => m.TermMasterId_BusinessName)"
                                                        data-info-content-description="@Html.DisplayFor(m => m.TermMasterId_BusinessDescription)"
                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.TermMasterId_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                                </button>
                                                <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#scholarshipTermMasterIdHistoryModal" tabindex="-1"
                                                        data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].TermMasterId_Status)"
                                                        data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                        data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].TermMasterId)">
                                                    <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                                </button>
                                                <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                            </div>
                                            <label class="control-label" for="TermMasterId">@Model.TermMasterId_BusinessName: </label>
                                            <select asp-for="TermMasterId" class="form-control" placeholder="@Model.TermMasterId_BusinessName" data-disabled="@(Model.TermMasterId_IsReadOnly.ToString())"></select>
                                            <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                            <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                            <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                            <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                            @Html.Raw(Model.TermMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                            <span asp-validation-for="TermMasterId" class="text-danger help-block"></span>
                                        </div>
                                    </div><!-- /.col-* -->
                                    <div class="col-xs-12 col-sm-6 col-lg-4">
                                        <!-- ScholarshipMasterId -->
                                        <div class='form-group @string.Format("status{0}", Model.ScholarshipMasterId_Status)'>
                                            <div class="pull-right">
                                                <input type="hidden" asp-for="ScholarshipMasterId_BusinessName" />
                                                <input type="hidden" asp-for="ScholarshipMasterId_BusinessDescription" />
                                                <input type="hidden" asp-for="ScholarshipMasterId_Source" />
                                                <input type="hidden" asp-for="ScholarshipMasterId_Status" />
                                                <input type="hidden" asp-for="ScholarshipMasterId_OriginalValue" />
                                                <input type="hidden" asp-for="ScholarshipCode" />
                                                <input type="hidden" asp-for="ScholarshipName" />
                                                <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                        data-info-content-name="@Html.DisplayFor(m => m.ScholarshipMasterId_BusinessName)"
                                                        data-info-content-description="@Html.DisplayFor(m => m.ScholarshipMasterId_BusinessDescription)"
                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.ScholarshipMasterId_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                                </button>
                                                <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#scholarshipScholarshipMasterIdHistoryModal" tabindex="-1"
                                                        data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipMasterId_Status)"
                                                        data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                        data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipMasterId)">
                                                    <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                                </button>
                                                <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                            </div>
                                            <label class="control-label" for="ScholarshipMasterId">@Model.ScholarshipMasterId_BusinessName: </label>
                                            @Html.DropDownListFor(
                                                m => m.ScholarshipMasterId,
                                                Model.Scholarships,
                                                htmlAttributes: new { @class = "form-control" }
                                            )
                                            <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                            <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                            <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                            <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                            @Html.Raw(Model.ScholarshipMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                            <span asp-validation-for="ScholarshipMasterId" class="text-danger help-block"></span>
                                        </div>
                                    </div><!-- /.col-* -->
                                    <div class="col-xs-12 col-sm-6 col-lg-4">
                                        <!-- ScholarshipDesignationMasterId -->
                                        <div class='form-group @string.Format("status{0}", Model.ScholarshipDesignationMasterId_Status)'>
                                            <div class="pull-right">
                                                <input type="hidden" asp-for="ScholarshipDesignationMasterId_BusinessName" />
                                                <input type="hidden" asp-for="ScholarshipDesignationMasterId_BusinessDescription" />
                                                <input type="hidden" asp-for="ScholarshipDesignationMasterId_Source" />
                                                <input type="hidden" asp-for="ScholarshipDesignationMasterId_Status" />
                                                <input type="hidden" asp-for="ScholarshipKFSAccount" />
                                                <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                        data-info-content-name="@Html.DisplayFor(m => m.ScholarshipDesignationMasterId_BusinessName)"
                                                        data-info-content-description="@Html.DisplayFor(m => m.ScholarshipDesignationMasterId_BusinessDescription)"
                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.ScholarshipDesignationMasterId_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                                </button>
                                                <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#scholarshipScholarshipDesignationMasterIdHistoryModal" tabindex="-1"
                                                        data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipDesignationMasterId_Status)"
                                                        data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                        data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipDesignationMasterId)">
                                                    <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                                </button>
                                                <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                            </div>
                                            <label class="control-label" for="ScholarshipDesignationMasterId">@Model.ScholarshipDesignationMasterId_BusinessName: </label>
                                            @Html.DropDownListFor(
                                                m => m.ScholarshipDesignationMasterId,
                                                Model.Designations,
                                                htmlAttributes: new { @class = "form-control", @data_disabled = (Model.ScholarshipDesignationMasterId_IsReadOnly.ToString()) }
                                            )
                                            <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                            <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                            <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                            <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                            @Html.Raw(Model.ScholarshipDesignationMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                            <span asp-validation-for="ScholarshipDesignationMasterId" class="text-danger help-block"></span>
                                        </div>
                                    </div><!-- /.col-* -->
                                    <div class="col-xs-12 col-sm-6 col-lg-4">
                                        <!-- ScholarshipDepartmentMasterId -->
                                        <div class='form-group @string.Format("status{0}", Model.ScholarshipDepartmentMasterId_Status)'>
                                            <div class="pull-right">
                                                <input type="hidden" asp-for="ScholarshipDepartmentMasterId_BusinessName" />
                                                <input type="hidden" asp-for="ScholarshipDepartmentMasterId_BusinessDescription" />
                                                <input type="hidden" asp-for="ScholarshipDepartmentMasterId_Source" />
                                                <input type="hidden" asp-for="ScholarshipDepartmentMasterId_Status" />
                                                <input type="hidden" asp-for="ScholarshipDepartmentCode" />
                                                <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                        data-info-content-name="@Html.DisplayFor(m => m.ScholarshipDepartmentMasterId_BusinessName)"
                                                        data-info-content-description="@Html.DisplayFor(m => m.ScholarshipDepartmentMasterId_BusinessDescription)"
                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.ScholarshipDepartmentMasterId_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                                </button>
                                                <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#scholarshipScholarshipDepartmentMasterIdHistoryModal" tabindex="-1"
                                                        data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipDepartmentMasterId_Status)"
                                                        data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                        data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipDepartmentMasterId)">
                                                    <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                                </button>
                                                <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                            </div>
                                            <label class="control-label" for="ScholarshipDepartmentMasterId">@Model.ScholarshipDepartmentMasterId_BusinessName: </label>
                                            @Html.DropDownListFor(
                                                m => m.ScholarshipDepartmentMasterId,
                                                Model.Departments,
                                                htmlAttributes: new { @class = "form-control", @data_disabled = (Model.ScholarshipDepartmentMasterId_IsReadOnly.ToString()) }
                                            )
                                            <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                            <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                            <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                            <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                            @Html.Raw(Model.ScholarshipDepartmentMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                            <span asp-validation-for="ScholarshipDepartmentMasterId" class="text-danger help-block"></span>
                                        </div>
                                    </div><!-- /.col-* -->
                                    <div class="col-xs-12 col-sm-6 col-lg-4">
                                        <!-- ScholarshipAmount -->
                                        <div class='form-group @string.Format("status{0}", Model.ScholarshipAmount_Status)'>
                                            <div class="pull-right">
                                                <input type="hidden" asp-for="ScholarshipAmount_BusinessName" />
                                                <input type="hidden" asp-for="ScholarshipAmount_BusinessDescription" />
                                                <input type="hidden" asp-for="ScholarshipAmount_Source" />
                                                <input type="hidden" asp-for="ScholarshipAmount_Status" />
                                                <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                        data-info-content-name="@Html.DisplayFor(m => m.ScholarshipAmount_BusinessName)"
                                                        data-info-content-description="@Html.DisplayFor(m => m.ScholarshipAmount_BusinessDescription)"
                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.ScholarshipAmount_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                                </button>
                                                <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#scholarshipScholarshipAmountHistoryModal" tabindex="-1"
                                                        data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipAmount_Status)"
                                                        data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                        data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].ScholarshipAmount)">
                                                    <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                                </button>
                                                <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                            </div>
                                            <label class="control-label" for="ScholarshipAmount">@Model.ScholarshipAmount_BusinessName: </label>
                                            <div class="input-group">
                                                <span class="input-group-addon" id="dollar-addon">$</span>
                                                <input asp-for="ScholarshipAmount" class="form-control currency" data-prefix="" placeholder="@Model.ScholarshipAmount_BusinessName" aria-describedby="dollar-addon" aria-label="Amount (to the nearest dollar)" />
                                            </div>
                                            <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                            <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                            <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                            <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                            @Html.Raw(Model.ScholarshipAmount_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                            <span asp-validation-for="ScholarshipAmount" class="text-danger help-block"></span>
                                        </div>
                                    </div><!-- /.col-* -->
                                </fieldset>

                            </div><!-- /.panel-body -->
                        </div><!-- /.panel-primary -->
                    </div><!-- /.panel-container -->

                </div><!-- /.panel-body -->

                <div class="panel-footer">
                    <button type="button" class="btn btn-link" title="Remove/Archive this Record" data-toggle="modal" data-target="#removeArchiveRecordModal">
                        <i role="img" class="fas fa-trash-alt">&nbsp;</i>Remove this Record
                    </button>
                    <button type="submit" id="btnResolve" class="btn btn-primary pull-right" title="Re-Validate Record" data-toggle="tooltip" formaction="@Url.Action("StudentScholarshipRevalidate", "Student")/">
                        <i role="img" class="fas fa-check">&nbsp;</i>Resolve Record
                    </button>
                    <button type="submit" id="btnSavePartial" class="btn btn-info pull-right" title="Save Partial Record" data-toggle="tooltip" formaction="@Url.Action("StudentScholarshipSave", "Student")/" disabled="disabled">
                        <i role="img" class="fas fa-save">&nbsp;</i>Save Partial Record
                    </button>
                    <a asp-area="" asp-controller="Student" asp-action="StudentScholarshipList" class="btn btn-link pull-right" title="Cancel Changes" data-toggle="tooltip"><i role="img" class="fas fa-times">&nbsp;</i>Discard</a>
                </div><!-- /.panel-footer -->
                <!-- Modal - Remove/Archive -->
                <div class="modal fade" id="removeArchiveRecordModal" tabindex="-1" role="dialog" aria-labelledby="removeArchiveRecordModal removeArchiveRecordModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h3 class="modal-title" id="removeArchiveRecordModalLabel">Remove @Model.StudentId (@Model.ScholarshipAmount)?</h3>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close" data-toggle="tooltip" title="Close"><i aria-hidden="true" class="ua-brand-x"></i></button>
                            </div>
                            <div class="modal-body">
                                <div class="row">
                                    <div class="col-sm-12">
                                        <p>Are you sure you want to remove this @Model.Integration record?</p>
                                        <ul>
                                            <li>Student Id: <strong>@Model.StudentId</strong></li>
                                            <li>Amount: <strong>@Model.ScholarshipAmount</strong></li>
                                        </ul>
                                    </div><!-- /.col-* -->
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-link pull-left" data-dismiss="modal" data-toggle="tooltip" title="Keep Record"><i role="img" class="fas fa-ban">&nbsp;</i> Cancel</button>
                                <button type="submit" class="btn btn-primary" title="Remove/Archive" formaction='@Url.Action("StudentIgnore", "Student", new { SystemId = Model.SystemId, IntegrationId = Model.IntegrationId, Id = Model.Id })'><i role="img" class="fas fa-trash-alt">&nbsp;</i> Remove/Archive</button>
                            </div>
                        </div><!-- /.modal-content -->
                    </div><!-- /.modal-dialog -->
                </div><!-- /.modal (Remove/Archive) -->

