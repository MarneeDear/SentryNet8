    <script>
        $(document).ready(function () {


            ////////////////////////////////////////////////////////////////////////////


            // StudentMasterId ---
            var StudentMasterIdDropDown = $('select#StudentMasterId');
            var studentMethod = '@Html.Raw(Url.Action("GetStudents", "DropDowns"))';

            $(StudentMasterIdDropDown).select2({
                selectOnClose: false, // select currently highlighted option when menu is closed
                closeOnSelect: true, // close menu once item is selected
                allowClear: true, // adds 'x' to clear selected option
                width: '100%',
                ajax: {
                    delay: 250,
                    url: studentMethod,
                    dataType: 'json',
                    contentType: "application/json; charset=utf-8",
                    data: function (params) {
                        var queryParams = {
                            term: params.term, // search term
                            _type: 'query_append',
                            selected: true,
                            page: params.page || 1
                        }
                        return queryParams;
                    },
                    processResults: function (data, params) {
                        params.page = params.page || 1;
                        return {
                            results: data,
                            pagination: {
                                more: (params.page * 5) < data.length
                            }
                        };
                    },
                    cache: true
                },
                placeholder: '-- Choose Student --',
                minimumInputLength: 3,
                //minimumResultsForSearch: 5,
                templateResult: function (data) {
                    var $result = $("<span></span>");
                    $result.text(data.text);
                    return $result;
                },
                templateSelection: function (data) {
                    return data.text;
                }
            });

            // Initialize select2 with Current Value
            var savedValue = '@Model.StudentId';
            //var savedValue = '@string.Format("{0} ({1})", Model.StudentId, Model.StudentId)';
            console.log('Selected StudentMasterId value: ' + savedValue);

            var masterId = @(Html.Raw(string.IsNullOrEmpty(Model.StudentMasterId) ? "null" : $"'{Model.StudentMasterId}'"));

            if (masterId != null) {
                var savedOption = new Option(savedValue, masterId, true);
                $(StudentMasterIdDropDown).append(savedOption).trigger('change');
            };

            // Get/Set Current Drop-Down Value:
            $(StudentMasterIdDropDown).on('change', function () {

                var currentStudentMasterIdSelectListValue = $(this).find(":selected").val();

                if (currentStudentMasterIdSelectListValue == null || currentStudentMasterIdSelectListValue == "") {
                    $('#StudentId').val(null);
                } else {
                    var url = '@Html.Raw(Url.Action("GetStudent", "Student"))' + '/' + currentStudentMasterIdSelectListValue;
                    $.getJSON(url, function (data) {
                        console.log(data);
                        console.log("success");
                        $('#StudentId').val(data.studentId);
                    });
                };
            });


            ////////////////////////////////////////////////////////////////////////////


            // ScholarshipMasterId ---
            var scholarshipMasterIdDropDown = $('#ScholarshipMasterId');
            $(scholarshipMasterIdDropDown).select2({
                selectOnClose: false, // select currently highlighted option when menu is closed
                closeOnSelect: true, // close menu once item is selected
                allowClear: true, // adds 'x' to clear selected option
                width: '100%',
            });
            // Get/Set Current Drop-Down Value:
            $(scholarshipMasterIdDropDown).on('change', function () {

                var url = '@Html.Raw(Url.Action("GetScholarship", "Student"))' + '/' + $(this).find(":selected").val();
                $.getJSON(url, function (data) {
                    console.log(data);
                    console.log("success");
                    $('#ScholarshipCode').val(data.uaCode);
                    $('#ScholarshipName').val(data.scholarshipName);
                });

                // get current value/text
                var currentScholarshipMasterIdSelectListValue = $(this).find(":selected").val();

                // set current value on hidden field
                if (currentScholarshipMasterIdSelectListValue !== "") {
                    $(scholarshipMasterIdDropDown).val(currentScholarshipMasterIdSelectListValue);
                } else {
                    $(scholarshipMasterIdDropDown).val("");
                };
            });


            ////////////////////////////////////////////////////////////////////////////


            // ScholarshipDesignation ---
            var scholarshipDesignationDropDown = $('#ScholarshipDesignationMasterId');
            $(scholarshipDesignationDropDown).select2({
                selectOnClose: false, // select currently highlighted option when menu is closed
