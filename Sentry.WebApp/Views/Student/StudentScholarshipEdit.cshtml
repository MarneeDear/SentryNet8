            // Get/Set Current Drop-Down Value:
            $(scholarshipDepartmentDropDown).on('change', function () {

                var url = '@Html.Raw(Url.Action("GetOrganizationalUnitDetailsByMasterId", "Student"))' + '/' + $(this).find(":selected").val();
                $.getJSON(url, function (data) {
                    console.log(data);
                    console.log("success");
                    $('#ScholarshipDepartmentCode').val(data.organizationalUnitCode);
