            // Initialize select2 with Current Value
            var savedValue = '@string.Format("{0} ({1})", Model.AcademicCareerCode, Model.TermCode)';
            console.log('Selected TermMasterId value: ' + savedValue);

            var masterId = @(Html.Raw(string.IsNullOrEmpty(Model.TermMasterId) ? "null" : $"'{Model.TermMasterId}'"));

