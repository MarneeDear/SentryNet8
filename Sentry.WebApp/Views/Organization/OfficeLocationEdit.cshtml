                                            <input type="hidden" asp-for="BuildingCode_FieldId" />
                                            <span asp-validation-for="BuildingCode" class="text-danger help-block"></span>
                                        </div>
                                    </div>
                                    <!-- /.col-* -->
                                </fieldset>

