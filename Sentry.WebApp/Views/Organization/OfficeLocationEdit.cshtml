    <script>
        $(document).ready(function () {

            // https://select2.org/
            // -------------------------------------------------

            // State ---
            $('#StateMasterId').select2({
                selectOnClose: true,
                width: '100%'
            });
            // Country ---
            $('#CountryMasterId').select2({
                selectOnClose: true,
                width: '100%'
            });


            // Get/Set Current Drop-Down Value:
            // -------------------------------------------------
            $('#StateMasterId').on('change', function () {
                // get current value/text
                var currentStateSelectListValue = $(this).find(":selected").text();
                //alert(currentStateSelectListValue);

                // set current value on hidden field '#State'
                if (currentStateSelectListValue !== "" && currentStateSelectListValue !== "-- Choose State --") {
                    $('#State').val(currentStateSelectListValue);
                } else {
                    $('#State').val("");
                };
            });

            $('#CountryMasterId').on('change', function () {
                // get current value/text
                var currentCountrySelectListValue = $(this).find(":selected").text();
                //alert(currentCountrySelectListValue);

                // set current value on hidden field '#Country'
                if (currentCountrySelectListValue !== "" && currentCountrySelectListValue !== "-- Choose Country --") {
                    $('#Country').val(currentCountrySelectListValue);
                } else {
                    $('#Country').val("");
                };
            });


            @*$('#btnSavePartial').on('click', function () {
                var urlSave = '@Url.Action("OfficeLocationSave", "Organization")/'
