                                                        data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                        data-info-content-sourcefieldid="@Html.DisplayFor(m => m.BuildingCode_Source)">
                                                    <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
