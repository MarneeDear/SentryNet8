@model Sentry.WebApp.ViewModels.GiftDisbursements.GiftDisbursementsViewModel

<!--From-->
<div class="">
    <h3>Request Details:</h3>
    <div class="col-xs-12 col-sm-12 col-lg-12">
        <dl class="dl-horizontal">
            <dt>Prepared By:</dt>
            <dd>@Model.PreparedByFirstName @Model.PreparedByLastName</dd>
            <dd>@Model.PreparedByEmail</dd>
            <dd>@Model.PreparedByJobTitle</dd>
            <dt>Prepared Date:</dt>
            <dd>@Model.CreatedOn</dd>
            <dt>Campus Approved Date:</dt>
            <dd>@Model.CampusApproveOn</dd>
            <dt>Division:</dt>
            <dd>@Model.DisbursementFrom.CollegeCode - @Model.DisbursementFrom.CollegeName</dd>
            <dt>Department:</dt>
            <dd>@Model.DisbursementFrom.DepartmentCode - @Model.DisbursementFrom.DepartmentName</dd>
            <dt>U of A Business-Related Purpose:</dt>
            <dd>
                @Model.DisbursementFrom.Purpose 
            </dd>
        </dl>
    </div>
</div>

