@section Scripts {

    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}

    <script>
        $(document).ready(function () {

            ////////////////////////////////////////////////////////////////////////////


            // ConstituentMasterId ---
            var ConstituentMasterIdDropDown = $('select#ConstituentMasterId');
            var personMethod = '@Html.Raw(Url.Action("GetConstituents", "DropDowns"))';

            $(ConstituentMasterIdDropDown).select2({
                selectOnClose: false, // select currently highlighted option when menu is closed
                closeOnSelect: true, // close menu once item is selected
                allowClear: true,
                width: '100%',
                ajax: {
                    delay: 250,
                    url: personMethod,
                    dataType: 'json',
                    contentType: "application/json; charset=utf-8",
                    data: function (params) {
                        var queryParams = {
                            term: params.term, // search term
                            _type: 'query_append',
                            selected: true,
                            page: params.page || 1
                        }
                        return queryParams;
                    },
                    processResults: function (data, params) {
                        params.page = params.page || 1;
                        return {
                            results: data,
                            pagination: {
                                more: (params.page * 5) < data.length
                            }
                        };
                    },
                    cache: true
                },
                placeholder: '-- Choose Constituent --',
                minimumInputLength: 3,
                //minimumResultsForSearch: 5,
                templateResult: function (data) {
                    var $result = $("<span></span>");
                    $result.text(data.text);
                    return $result;
                },
                templateSelection: function (data) {
                    return data.text;
                }
            });

            // Initialize select2 with Current Value
            var savedValue = '@(string.Format("{0}{1}{2}", (string.IsNullOrEmpty(Model.LastName) ? string.Empty : $"{Model.LastName}, "), Model.FirstName, (string.IsNullOrEmpty(Model.UAPersonId) ? string.Empty : $" ({Model.UAPersonId})")))'
            console.log('Selected Constituent value: ' + savedValue);

            var masterId = @(Html.Raw(string.IsNullOrEmpty(Model.ConstituentMasterId) ? "null" : $"'{Model.ConstituentMasterId}'"));

            if (masterId != null) {
                var savedOption = new Option(savedValue, masterId, true);
                $(ConstituentMasterIdDropDown).append(savedOption).trigger('change');
            };

            // Get/Set Current Drop-Down Value:
            $(ConstituentMasterIdDropDown).on('change', function () {

                var currentConstituentMasterIdSelectListValue = $(this).find(":selected").val();

                if (currentConstituentMasterIdSelectListValue == null || currentConstituentMasterIdSelectListValue == "") {
                    $('#FirstName').val(null);
                    $('#LastName').val(null);
                    $('#UAPersonId').val(null);
                    $('#ConstituentSourceSystemRecordId').val(null);
                } else {
                    var url = '@Html.Raw(Url.Action("GetConstituent", "Constituent"))' + '/' + currentConstituentMasterIdSelectListValue;
                    $.getJSON(url, function (data) {
                        console.log(data);
                        console.log("success");
                        $('#FirstName').val(data.firstName);
                        $('#LastName').val(data.lastName);
                        $('#UAPersonId').val(data.uaPersonId);
                        $('#ConstituentSourceSystemRecordId').val(data.constituentSourceSystemRecordId);
                    });
                };
            });


            ////////////////////////////////////////////////////////////////////////////


            // EmailAddress ---
            var EmailAddressInput = $('input#EmailAddress');
            $(EmailAddressInput).on('keyup change propertychange paste input', function () {

                $('#EmailAddressMasterId').val(null);
                
            });


            ////////////////////////////////////////////////////////////////////////////


            // EmailAddressUseTypeMasterId ---
            var EmailAddressUseTypeMasterIdDropDown = $('select#EmailAddressUseTypeMasterId');
            $(EmailAddressUseTypeMasterIdDropDown).select2({
                selectOnClose: false, // select currently highlighted option when menu is closed
                closeOnSelect: true, // close menu once item is selected
                allowClear: true,
                placeholder: '-- Choose Use Type --',
                width: '100%'
            });

            var clearButtonClicked = false;
            var clearButton = $('.select2-selection__clear');
            $(clearButton).on('click', function () {
                clearButtonClicked = true;
                console.log('clearButton clicked, and its value is ' + clearButtonClicked);
                $(this).parents('.form-group').find('.form-control').val(null).trigger('change');
                $('#EmailAddressUseType').val(null);
                $('#EmailAddressUseTypeSourceSystemRecordId').val(null);
            });

            // Get/Set Current Drop-Down Value:
            $(EmailAddressUseTypeMasterIdDropDown).on('change', function () {

                var currentEmailAddressUseTypeMasterIdSelectListValue = $(this).find(":selected").val();

                if (currentEmailAddressUseTypeMasterIdSelectListValue == "" || currentEmailAddressUseTypeMasterIdSelectListValue == null) {
                    $('#EmailAddressUseType').val(null);
                    $('#EmailAddressUseTypeSourceSystemRecordId').val(null);
                } else {
                    var url = '@Html.Raw(Url.Action("GetEmailUseType", "Constituent"))' + '/' + currentEmailAddressUseTypeMasterIdSelectListValue;
                    $.getJSON(url, function (data) {
                        console.log(data);
                        console.log("success");
                        $('#EmailAddressUseType').val(data.emailAddressUseType);
                        $('#EmailAddressUseTypeSourceSystemRecordId').val(data.emailAddressUseTypeSourceSystemRecordId);
                    });
                };
            });


            ////////////////////////////////////////////////////////////////////////////


            // For use in the external javascript file (site.js) ::
            var systemVariableName = '@Model.System';
            window.systemVariableName = '@Model.System';
            console.log("Source System: " + window.systemVariableName);


            // --------------------------------------------------------------



            var form = $('.content-container > form');
            var formGroup = $(':input').parent('.form-group');
            var editedClass = 'fa-pen-square';

            // Get Current Form Field Value:
            // -------------------------------------------------
            $(form).on('keyup change paste', ':input', function () {

                var formField = $(this);
                var thisFormFieldsValue = $(this).val();
                //console.log('form field value: ' + thisFormFieldsValue);

                return (thisFormFieldsValue);
                console.log(thisFormFieldsValue);

            });


            var dfOptions = {
                denoteDirtyForm: true,
                denoteDirtyOptions: true,
                dirtyOptionClass: "dirtyChoice",
                trimText: true,
                textboxContext: 'parent',

                preFieldChangeCallback: function (originalValue) {
                    //$("#callbackBox").html("[preFieldChangeCallback] Original value of field '" + $(this).attr("name") + "' is: " + originalValue + "<br />");
                },
                fieldChangeCallback: function (originalValue, result) {
                    //$("#callbackBox").append("[fieldChange callback] Original value of field  '" + $(this).attr("name") + "' is: " + originalValue + ".  Dirty status is " + result + "<br />");
                },
                formChangeCallback: function (result, dirtyFieldsArray) {
                    //var recordName = "@Model.EmailAddress";
                    var dirtyFieldParentPanelTitle = $('.form-group.dirtyField').parents('.panel').find('.updatedFormAlert');
                    var cleanFieldParentPanelTitle = $('.form-group:not(".dirtyField")').parents('.panel').find('.updatedFormAlert');
                    if (result) { // Dirty
                        //$("#callbackBox").append("[formChangeCallback] Form is dirty. Dirty fields are: " + dirtyFieldsArray + "<br />");
                        $(this).parents('.form-group').addClass('dirtyField').addClass('has-feedback');
                        $(this).find("#btnResolve").removeAttr("disabled");
                        //$(dirtyFieldParentPanelTitle).removeClass("hide").html("*" + recordName + "'s record has been modified*");
                        $(dirtyFieldParentPanelTitle).removeClass("hide").html("*Save your changes*");
                        $('#btnSavePartial').removeAttr('disabled');
                        $('#IsChanged').val(true);

                        // Validate Drop-down form fields:
                        $("select").on("select2:close", function (e) {
                            $(this).valid();
                            $(this).parents('.form-group').addClass('dirtyField').addClass('has-feedback');
                            $('.form-group.dirtyField').parents('.panel').find('.updatedFormAlert').removeClass("hide").html("*Save your changes*");
                        });
                    }
                    else { // Clean
                        //$("#callbackBox").append("[formChangeCallback] Form is clean.<br />");
                        $(this).parents('.form-group').removeClass('dirtyField has-feedback');
                        $(this).find("#btnSavePartial").attr("disabled", "disabled");
                        $('#IsChanged').val(false);

                        $(cleanFieldParentPanelTitle).addClass("hide").html("");
                        // Validate Drop-down form fields:
                        $("select").on("select2:close", function (e) {
                            $(this).valid();
                            $(this).parents('.form-group').removeClass('dirtyField has-feedback');
                            $('.form-group:not(".dirtyField")').parents('.panel').find('.updatedFormAlert').addClass("hide").html("");
                        });
                    }
                    //Scrolls the logging div to the bottom
                    //var objDiv = document.getElementById("callbackBox");
                    //objDiv.scrollTop = objDiv.scrollHeight;
                }
            };

            $(form).dirtyFields(dfOptions);


        });
    </script>

}