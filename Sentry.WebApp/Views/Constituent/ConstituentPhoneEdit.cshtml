@model Sentry.WebApp.ViewModels.ConstituentPhoneViewModel
@using Sentry.WebApp.Extensions;

@{
    ViewData["Title"] = Model.Title;
}

@if (Model != null)
{
    <ul class="page-title">
        <li>
            <h2>
                Date:
                <span title="Date of Integration: @Model.IntegrationDate"> @Html.Raw(Model.CreatedOnDT.HasValue ? Model.CreatedOnDT.Value.ToString("MM/dd/yyyy") : "N/A")</span>
            </h2>
        </li>
        <li>
            <h2>
                Source:
                <span title="Source System">@Html.Raw(Model.System ?? "N/A")</span>
            </h2>
        </li>
        <li>
            <h2>
                Source Record Id:
                @if (Model.System == "Lynx")
                {
                    <a href="@Model.SourceRecordUrl" title="View this record in @Model.System" class="" target="_blank">
                        <span>@Html.Raw(Model.SourceRecordId ?? "N/A")</span>
                    </a>
                }
                else
                {
                    @*<span title="Source System Record Id">@(Model.SourceRecordId != "" || Model.SourceRecordId != null ? Html.Raw(Model.SourceRecordId) : Html.Raw("N/A"))</span>*@
                    <span title="Source System Record Id">@Html.Raw(Model.SourceRecordId ?? "N/A")</span>
                }
            </h2>
        </li>
    </ul>

    <ol class="breadcrumb">
        <li><a asp-area="" asp-controller="Home" asp-action="Index">Sentry Home</a></li>
        <li><a asp-area="" asp-controller="Constituent" asp-action="Index">Constituent Dashboard</a></li>
        <li><a asp-area="" asp-controller="Constituent" asp-action="ConstituentPhoneList">Record List</a></li>
        <li class="active">Record Details</li>
    </ol>

    <div id="content-container" class="content-container" role="main">
        <form asp-action="ConstituentPhoneEdit" id="ConstituentPhoneEditForm">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="SystemId" />
            <input type="hidden" asp-for="System" />
            <input type="hidden" asp-for="SourceRecordId" />
            <input type="hidden" asp-for="IntegrationId" />
            <input type="hidden" asp-for="Integration" />
            <input type="hidden" asp-for="CreatedOnDT" />
            <input type="hidden" asp-for="IsChanged" />
            @for (int i = 0; i <= Model.HistoryData.Count - 1; i++)
            {
                @Html.HiddenFor(m => m.HistoryData[i].HistoryDate)
            }

        <div class="panel panel-secondary">
            <div class="panel-heading">
                <h3 class="panel-title">@Model.Title for: <span class="record-text h3">@Model.PhoneNumber / @Model.Extension / @Model.PhoneLineType</span> <span class="updatedFormAlert label label-warning hide">Modified</span></h3>
                <div class="panel-toolbar">
                    <a href="#constituentPhoneSourceDataModal" class="btn btn-sm btn-link" target="_blank" data-toggle="modal" title="View Source Data"><i role="img" class="fas fa-database fa-sm">&nbsp;</i>Source Data</a>
                </div>
            </div>
            <div class="panel-body">

                <div class="col-sm-12 col-md-12 panel-container">
                    <div class="panel panel-secondary">
                        <div class="panel-heading">
                            <h4 class="panel-title">Phone Details:</h4>
                            <div class="panel-toolbar">
                                <button class="btn btn-sm btn-link infoLegend" data-toggle="tooltip" type="button" aria-expanded="false">History Legend <i role="img" class="fas fa-question-circle"></i></button>
                            </div>
                        </div>
                        <div class="panel-body">

                            <fieldset>
                                <legend class="sr-only">Degree Details:</legend>
                                <div class="col-xs-12 col-sm-6 col-lg-4">
                                    <!-- ConstituentMasterId -->
                                    <div class='form-group @string.Format("status{0}", Model.ConstituentMasterId_Status)'>
                                        <div class="pull-right">
                                            <input type="hidden" asp-for="ConstituentMasterId_BusinessName" />
                                            <input type="hidden" asp-for="ConstituentMasterId_BusinessDescription" />
                                            <input type="hidden" asp-for="ConstituentMasterId_Source" />
                                            <input type="hidden" asp-for="ConstituentMasterId_Status" />
                                            <input type="hidden" asp-for="ConstituentSourceSystemRecordId" />
                                            <input type="hidden" asp-for="FirstName" />
                                            <input type="hidden" asp-for="LastName" />
                                            <input type="hidden" asp-for="UAPersonId" />
                                            <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                    data-info-content-name="@Html.DisplayFor(m => m.ConstituentMasterId_BusinessName)"
                                                    data-info-content-description="@Html.DisplayFor(m => m.ConstituentMasterId_BusinessDescription)"
                                                    data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                    data-info-content-sourcefieldid="@Html.DisplayFor(m => m.ConstituentMasterId_Source)">
                                                <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                            </button>
                                            <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#constituentPhoneConstituentMasterIdHistoryModal" tabindex="-1"
                                                    data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].ConstituentMasterId_Status)"
                                                    data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                    data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].ConstituentMasterId)">
                                                <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                            </button>
                                            <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                        </div>
                                        <label class="control-label" for="ConstituentMasterId">@Model.ConstituentMasterId_BusinessName: </label>
                                        <select asp-for="ConstituentMasterId" class="form-control" placeholder="@Model.ConstituentMasterId_BusinessName" data-disabled="@(Model.ConstituentMasterId_IsReadOnly.ToString())"></select>
                                        <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                        <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                        <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                        <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                        @Html.Raw(Model.ConstituentMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                        <span asp-validation-for="ConstituentMasterId" class="text-danger help-block"></span>
                                    </div>
                                </div><!-- /.col-* -->
                                <div class="col-xs-12 col-sm-6 col-lg-4">
                                    <!-- PhoneNumber -->
                                    <div class='form-group @string.Format("status{0}", Model.PhoneNumber_Status)'>
                                        <div class="pull-right">
                                            <input type="hidden" asp-for="PhoneNumber_BusinessName" />
                                            <input type="hidden" asp-for="PhoneNumber_BusinessDescription" />
                                            <input type="hidden" asp-for="PhoneNumber_Source" />
                                            <input type="hidden" asp-for="PhoneNumber_Status" />
                                            <input type="hidden" asp-for="PhoneMasterId" />
                                            <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                    data-info-content-name="@Html.DisplayFor(m => m.PhoneNumber_BusinessName)"
                                                    data-info-content-description="@Html.DisplayFor(m => m.PhoneNumber_BusinessDescription)"
                                                    data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                    data-info-content-sourcefieldid="@Html.DisplayFor(m => m.PhoneNumber_Source)">
                                                <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                            </button>
                                            <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#constituentPhonePhoneNumberHistoryModal" tabindex="-1"
                                                    data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].PhoneNumber_Status)"
                                                    data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                    data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].PhoneNumber)">
                                                <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                            </button>
                                            <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                        </div>
                                        <label class="control-label" for="PhoneMasterId">@Model.PhoneNumber_BusinessName: </label>
                                        <input asp-for="PhoneNumber" class="form-control" placeholder="@Model.PhoneMasterId_BusinessName" data-disabled="@(Model.PhoneNumber_IsReadOnly.ToString())" />
                                        <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                        <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                        <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                        <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                        @Html.Raw(Model.PhoneNumber_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                        <span asp-validation-for="PhoneNumber" class="text-danger help-block"></span>
                                                                                <div class="lg-12">
                                        <div class="form-group">
                                                <div class="checkbox checkbox-primary flex-firstchild">
                                                    <input type="checkbox" asp-for="TempPhoneIsPrimary">
                                                    <label for="TempPhoneIsPrimary">Primary Phone</label>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div><!-- /.col-* -->
                                <div class="col-xs-12 col-sm-6 col-lg-4">
                                    <!-- Extension -->
                                    <div class='form-group @string.Format("status{0}", Model.Extension_Status)'>
                                        <div class="pull-right">
                                            <input type="hidden" asp-for="Extension_BusinessName" />
                                            <input type="hidden" asp-for="Extension_BusinessDescription" />
                                            <input type="hidden" asp-for="Extension_Source" />
                                            <input type="hidden" asp-for="Extension_Status" />
                                            <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                    data-info-content-name="@Html.DisplayFor(m => m.Extension_BusinessName)"
                                                    data-info-content-description="@Html.DisplayFor(m => m.Extension_BusinessDescription)"
                                                    data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                    data-info-content-sourcefieldid="@Html.DisplayFor(m => m.Extension_Source)">
                                                <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                            </button>
                                            <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#constituentPhoneExtensionHistoryModal" tabindex="-1"
                                                    data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].Extension_Status)"
                                                    data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                    data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].Extension)">
                                                <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                            </button>
                                            <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                        </div>
                                        <label class="control-label" for="Extension">@Model.Extension_BusinessName: </label>
                                        <input asp-for="Extension" class="form-control numeric" placeholder="@Model.Extension_BusinessName" data-disabled="@(Model.Extension_IsReadOnly.ToString())" />
                                        <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                        <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                        <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                        <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                        @Html.Raw(Model.Extension_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                        <span asp-validation-for="Extension" class="text-danger help-block"></span>
                                    </div>
                                </div><!-- /.col-* -->
                                <div class="col-xs-12 col-sm-6 col-lg-4">
                                    <!-- CountryMasterId -->
                                    <div class='form-group @string.Format("status{0}", Model.CountryMasterId_Status)'>
                                        <div class="pull-right">
                                            <input type="hidden" asp-for="CountryMasterId_BusinessName" />
                                            <input type="hidden" asp-for="CountryMasterId_BusinessDescription" />
                                            <input type="hidden" asp-for="CountryMasterId_Source" />
                                            <input type="hidden" asp-for="CountryMasterId_Status" />
                                            <input type="hidden" asp-for="CountrySourceSystemRecordId" />
                                            <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                    data-info-content-name="@Html.DisplayFor(m => m.CountryMasterId_BusinessName)"
                                                    data-info-content-description="@Html.DisplayFor(m => m.CountryMasterId_BusinessDescription)"
                                                    data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                    data-info-content-sourcefieldid="@Html.DisplayFor(m => m.CountryMasterId_Source)">
                                                <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                            </button>
                                            <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#constituentPhoneCountryMasterIdHistoryModal" tabindex="-1"
                                                    data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].CountryMasterId_Status)"
                                                    data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                    data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].CountryMasterId)">
                                                <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                            </button>
                                            <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                        </div>
                                        <label class="control-label" for="CountryMasterId">@Model.CountryMasterId_BusinessName Code: </label>
                                        @Html.DropDownListFor(
                                            m => m.CountryMasterId,
                                            Model.CountryList,
                                            htmlAttributes: new { @class = "form-control", @data_disabled = (Model.CountryMasterId_IsReadOnly.ToString()) }
                                        )
                                        <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                        <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                        <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                        <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                        @Html.Raw(Model.CountryMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                        <span asp-validation-for="CountryMasterId" class="text-danger help-block"></span>
                                    </div>
                                </div><!-- /.col-* -->
                                <div class="col-xs-12 col-sm-6 col-lg-4">
                                    <!-- PhoneLineTypeMasterId -->
                                    <div class='form-group @string.Format("status{0}", Model.PhoneLineTypeMasterId_Status)'>
                                        <div class="pull-right">
                                            <input type="hidden" asp-for="PhoneLineTypeMasterId_BusinessName" />
                                            <input type="hidden" asp-for="PhoneLineTypeMasterId_BusinessDescription" />
                                            <input type="hidden" asp-for="PhoneLineTypeMasterId_Source" />
                                            <input type="hidden" asp-for="PhoneLineTypeMasterId_Status" />
                                            <input type="hidden" asp-for="PhoneLineType" />
                                            <input type="hidden" asp-for="PhoneLineTypeSourceSystemRecordId" />
                                            <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                    data-info-content-name="@Html.DisplayFor(m => m.PhoneLineTypeMasterId_BusinessName)"
                                                    data-info-content-description="@Html.DisplayFor(m => m.PhoneLineTypeMasterId_BusinessDescription)"
                                                    data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                    data-info-content-sourcefieldid="@Html.DisplayFor(m => m.PhoneLineTypeMasterId_Source)">
                                                <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                            </button>
                                            <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#constituentPhonePhoneLineTypeMasterIdHistoryModal" tabindex="-1"
                                                    data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].PhoneLineTypeMasterId_Status)"
                                                    data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                    data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].PhoneLineTypeMasterId)">
                                                <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                            </button>
                                            <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                        </div>
                                        <label class="control-label" for="PhoneLineTypeMasterId">@Model.PhoneLineTypeMasterId_BusinessName: </label>
                                        @Html.DropDownListFor(
                                            m => m.PhoneLineTypeMasterId,
                                            Model.PhoneLineTypeList,
                                            htmlAttributes: new { @class = "form-control", @data_disabled = (Model.PhoneLineTypeMasterId_IsReadOnly.ToString()) }
                                        )
                                        <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                        <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                        <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                        <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                        @Html.Raw(Model.PhoneLineTypeMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                        <span asp-validation-for="PhoneLineTypeMasterId" class="text-danger help-block"></span>
                                    </div>
                                </div><!-- /.col-* -->
                                <div class="col-xs-12 col-sm-6 col-lg-4">
                                    <!-- PhoneUseTypeMasterId -->
                                    <div class='form-group @string.Format("status{0}", Model.PhoneUseTypeMasterId_Status)'>
                                        <div class="pull-right">
                                            <input type="hidden" asp-for="PhoneUseTypeMasterId_BusinessName" />
                                            <input type="hidden" asp-for="PhoneUseTypeMasterId_BusinessDescription" />
                                            <input type="hidden" asp-for="PhoneUseTypeMasterId_Source" />
                                            <input type="hidden" asp-for="PhoneUseTypeMasterId_Status" />
                                            <input type="hidden" asp-for="PhoneUseType" />
                                            <input type="hidden" asp-for="PhoneUseTypeSourceSystemRecordId" />
                                            <button type="button" class="btn btn-xs infoPerField" rel="popover" tabindex="-1"
                                                    data-info-content-name="@Html.DisplayFor(m => m.PhoneUseTypeMasterId_BusinessName)"
                                                    data-info-content-description="@Html.DisplayFor(m => m.PhoneUseTypeMasterId_BusinessDescription)"
                                                    data-info-content-source="@Html.DisplayFor(m => m.System)"
                                                    data-info-content-sourcefieldid="@Html.DisplayFor(m => m.PhoneUseTypeMasterId_Source)">
                                                <i role="img" class="fas fa-info-circle fa-sm">&nbsp;</i><span>Info</span>
                                            </button>
                                            <button type="button" class="btn btn-xs historyPerField" title="Field History" data-toggle="modal" data-target="#constituentPhonePhoneUseTypeMasterIdHistoryModal" tabindex="-1"
                                                    data-history-content-status="@Html.DisplayFor(m => m.HistoryData[0].PhoneUseTypeMasterId_Status)"
                                                    data-history-content-date="@Html.DisplayFor(m => m.HistoryData[0].HistoryDate)"
                                                    data-history-content-value="@Html.DisplayFor(m => m.HistoryData[0].PhoneUseTypeMasterId)">
                                                <i role="img" class="fas fa-history fa-sm">&nbsp;</i><span>History</span>
                                            </button>
                                            <button type="button" class="btn btn-xs form-error text-danger hide" rel="popover" title="Field Errors:" tabindex="-1"><i role="img" class="fas fa-exclamation-triangle fa-sm">&nbsp;</i><span>Error</span></button>
                                        </div>
                                        <label class="control-label" for="PhoneUseTypeMasterId">@Model.PhoneUseTypeMasterId_BusinessName: </label>
                                        @Html.DropDownListFor(
                                            m => m.PhoneUseTypeMasterId,
                                            Model.PhoneUseTypeList,
                                            htmlAttributes: new { @class = "form-control", @data_disabled = (Model.PhoneUseTypeMasterId_IsReadOnly.ToString()) }
                                        )
                                        <span class="fa fa-cog form-control-feedback" aria-hidden="true" title="Enriched"></span>
                                        <span class="fa fa-pen-square form-control-feedback" aria-hidden="true" title="Changed"></span>
                                        <span class="fa fa-check-square form-control-feedback" aria-hidden="true" title="Good"></span>
                                        <span class="fa fa-exclamation-triangle form-control-feedback" aria-hidden="true" title="Bad"></span>
                                        @Html.Raw(Model.PhoneUseTypeMasterId_IsReadOnly ? $"<span class='fa fa-exclamation-triangle form-control-feedback' aria-hidden='true' title='Not Provided in {Model.System}'></span>" : "")
                                        <span asp-validation-for="PhoneUseTypeMasterId" class="text-danger help-block"></span>
                                    </div>
                                </div><!-- /.col-* -->
                            </fieldset>

                        </div><!-- /.panel-body -->
                    </div><!-- /.panel-primary -->
                </div><!-- /.panel-container -->

            </div><!-- /.panel-body -->

            <div class="panel-footer" style="height: 100px">
                <button type="button" class="btn btn-link @(Model.PhoneNumber_Status == "Bad" ? "" : "d-none")" title="Deactivate in @Model.System" data-toggle="modal" data-target="#warningSourceModal">
                        <i role="img" class="fas fa-trash-alt">&nbsp;</i>Deactivate in @Model.System
                </button>
                <button type="submit" id="btnResolve" class="btn btn-primary pull-right" title="Re-Validate Record" data-toggle="tooltip" formaction="@Url.Action("ConstituentPhoneRevalidate", "Constituent")/">
                    <i role="img" class="fas fa-check">&nbsp;</i>Resolve Record
                </button>
                <button type="submit" id="btnSavePartial" class="btn btn-info pull-right" title="Save Partial Record" data-toggle="tooltip" formaction="@Url.Action("ConstituentPhoneSave", "Constituent")/" disabled="disabled">
                    <i role="img" class="fas fa-save">&nbsp;</i>Save Partial Record
                </button>
                <a asp-area="" asp-controller="Constituent" asp-action="ConstituentPhoneList" class="btn btn-link pull-right" title="Cancel Changes" data-toggle="tooltip"><i role="img" class="fas fa-times">&nbsp;</i>Discard</a>
            </div><!-- /.panel-footer -->
            <!-- Modal - Remove/Archive -->
            <div class="modal fade" id="removeArchiveRecordModal" tabindex="-1" role="dialog" aria-labelledby="removeArchiveRecordModal removeArchiveRecordModalLabel" aria-hidden="true">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h3 class="modal-title" id="removeArchiveRecordModalLabel">Remove This @Model.Integration Record?</h3>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close" data-toggle="tooltip" title="Close"><i aria-hidden="true" class="ua-brand-x"></i></button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col-sm-12">
                                    <p>Are you sure you want to remove this @Model.Integration record?</p>
                                    <ul>
                                        <li>Phone Number: <strong>@Model.PhoneNumber</strong></li>
                                        <li>Country Code: <strong>@Model.CountrySourceSystemRecordId</strong></li>
                                        <li>Extension: <strong>@Model.Extension</strong></li>
                                        <li>Line Type: <strong>@Model.PhoneLineType</strong></li>
                                        <li>Use Type: <strong>@Model.PhoneUseType</strong></li>
                                    </ul>
                                </div><!-- /.col-* -->
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-link pull-left" data-dismiss="modal" data-toggle="tooltip" title="Keep Record"><i role="img" class="fas fa-ban">&nbsp;</i> Cancel</button>
                            <button type="submit" class="btn btn-primary" title="Remove/Archive" formaction='@Url.Action("ConstituentPhoneIgnore", "Constituent", new { SystemId = Model.SystemId, IntegrationId = Model.IntegrationId, Id = Model.Id })'><i role="img" class="fas fa-trash-alt">&nbsp;</i> Remove/Archive</button>
                        </div>
